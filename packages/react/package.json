{
  "name": "@ldesign/engine-react",
  "version": "0.3.0",
  "description": "React adapter for LDesign Engine - Provides React-specific integration for the universal application engine",
  "keywords": [
    "react",
    "engine",
    "adapter",
    "plugin",
    "middleware",
    "hooks",
    "typescript"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./adapters/*": {
      "types": "./es/adapters/*.d.ts",
      "import": "./es/adapters/*.js",
      "require": "./lib/adapters/*.cjs"
    },
    "./core/*": {
      "types": "./es/core/*.d.ts",
      "import": "./es/core/*.js",
      "require": "./lib/core/*.cjs"
    },
    "./hooks/*": {
      "types": "./es/hooks/*.d.ts",
      "import": "./es/hooks/*.js",
      "require": "./lib/hooks/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "dev": "ldesign-builder build --watch",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ldesign/engine-core": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@ldesign/router": "workspace:*",
    "@ldesign/router-react": "workspace:*",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.7.3"
  },
  "optionalDependencies": {
    "@ldesign/router": "workspace:*",
    "@ldesign/router-react": "workspace:*"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  }
}