{
  "name": "@ldesign/engine-react",
  "version": "0.2.0",
  "description": "React adapter for @ldesign/engine-core",
  "keywords": [
    "react",
    "engine",
    "plugin",
    "middleware",
    "typescript"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./components": {
      "types": "./es/components/index.d.ts",
      "import": "./es/components/index.js",
      "require": "./lib/components/index.cjs"
    },
    "./components/*": {
      "types": "./es/components/*.d.ts",
      "import": "./es/components/*.js",
      "require": "./lib/components/*.cjs"
    },
    "./hooks": {
      "types": "./es/hooks/index.d.ts",
      "import": "./es/hooks/index.js",
      "require": "./lib/hooks/index.cjs"
    },
    "./hooks/*": {
      "types": "./es/hooks/*.d.ts",
      "import": "./es/hooks/*.js",
      "require": "./lib/hooks/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib"
  ],
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs,dts",
    "dev": "ldesign-builder build -f esm,cjs,dts --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ldesign/engine-core": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@ldesign/engine-core": "workspace:*"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "eslint": "^9.18.0"
  }
}