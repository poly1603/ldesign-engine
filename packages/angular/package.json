{
  "name": "@ldesign/engine-angular",
  "version": "0.3.0",
  "description": "Angular adapter for LDesign Engine - Provides Angular-specific integration for the universal application engine",
  "keywords": [
    "angular",
    "engine",
    "adapter",
    "plugin",
    "middleware",
    "rxjs",
    "dependency-injection",
    "typescript"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./adapters/*": {
      "types": "./es/adapters/*.d.ts",
      "import": "./es/adapters/*.js",
      "require": "./lib/adapters/*.cjs"
    },
    "./core/*": {
      "types": "./es/core/*.d.ts",
      "import": "./es/core/*.js",
      "require": "./lib/core/*.cjs"
    },
    "./services/*": {
      "types": "./es/services/*.d.ts",
      "import": "./es/services/*.js",
      "require": "./lib/services/*.cjs"
    },
    "./utils/*": {
      "types": "./es/utils/*.d.ts",
      "import": "./es/utils/*.js",
      "require": "./lib/utils/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "dev": "ldesign-builder build --watch",
    "clean": "rimraf es lib dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ldesign/engine-core": "workspace:*",
    "rxjs": "^7.8.1"
  },
  "peerDependencies": {
    "@angular/core": "^19.0.0",
    "@angular/common": "^19.0.0"
  },
  "devDependencies": {
    "@angular/core": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@ldesign/builder": "workspace:*",
    "@ldesign/router": "workspace:*",
    "@ldesign/router-angular": "workspace:*",
    "typescript": "^5.7.3",
    "zone.js": "^0.15.1"
  },
  "optionalDependencies": {
    "@ldesign/router": "workspace:*",
    "@ldesign/router-angular": "workspace:*"
  }
}