{
  "name": "@ldesign/engine-preact",
  "version": "0.1.0",
  "description": "Preact adapter for LDesign Engine",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "pnpm run clean && tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["ldesign", "engine", "preact", "adapter"],
  "author": "LDesign Team",
  "license": "MIT",
  "peerDependencies": {
    "preact": "^10.0.0",
    "@ldesign/engine-core": "workspace:*"
  },
  "devDependencies": {
    "preact": "^10.24.3",
    "@ldesign/engine-core": "workspace:*",
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0",
    "jsdom": "^25.0.0"
  },
  "scripts": {
    "build": "pnpm run clean && tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run"
  }
}

