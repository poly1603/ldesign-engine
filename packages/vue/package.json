{
  "name": "@ldesign/engine-vue",
  "version": "0.2.0",
  "description": "Vue3 adapter for @ldesign/engine-core",
  "keywords": [
    "vue3",
    "engine",
    "plugin",
    "middleware",
    "typescript"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./adapter": {
      "types": "./es/adapter/index.d.ts",
      "import": "./es/adapter/index.js",
      "require": "./lib/adapter/index.cjs"
    },
    "./adapter/*": {
      "types": "./es/adapter/*.d.ts",
      "import": "./es/adapter/*.js",
      "require": "./lib/adapter/*.cjs"
    },
    "./composables": {
      "types": "./es/composables/index.d.ts",
      "import": "./es/composables/index.js",
      "require": "./lib/composables/index.cjs"
    },
    "./composables/*": {
      "types": "./es/composables/*.d.ts",
      "import": "./es/composables/*.js",
      "require": "./lib/composables/*.cjs"
    },
    "./directives": {
      "types": "./es/directives/index.d.ts",
      "import": "./es/directives/index.js",
      "require": "./lib/directives/index.cjs"
    },
    "./directives/*": {
      "types": "./es/directives/*.d.ts",
      "import": "./es/directives/*.js",
      "require": "./lib/directives/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib"
  ],
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs,umd,dts",
    "dev": "ldesign-builder build -f esm,cjs,umd,dts --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint . --fix",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@ldesign/core": "workspace:*",
    "@ldesign/engine-core": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.3.0",
    "@ldesign/engine-core": "workspace:*"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.18",
    "vue-tsc": "^3.0.5",
    "eslint": "^9.18.0"
  }
}