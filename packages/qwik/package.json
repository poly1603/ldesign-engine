{
  "name": "@ldesign/engine-qwik",
  "version": "0.2.0",
  "description": "Qwik adapter for LDesign Engine",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./adapter": {
      "types": "./dist/adapter/qwik-adapter.d.ts",
      "import": "./dist/adapter/qwik-adapter.js",
      "require": "./dist/adapter/qwik-adapter.cjs"
    },
    "./hooks": {
      "types": "./dist/hooks/use-engine.d.ts",
      "import": "./dist/hooks/use-engine.js",
      "require": "./dist/hooks/use-engine.cjs"
    }
  },
  "files": [
    "dist",
    "es",
    "lib",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist es lib",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest"
  },
  "keywords": [
    "ldesign",
    "engine",
    "qwik",
    "adapter",
    "framework"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "peerDependencies": {
    "@builder.io/qwik": "^1.0.0",
    "@ldesign/engine-core": "workspace:*"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.9.1",
    "@ldesign/engine-core": "workspace:*",
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/engine.git",
    "directory": "packages/qwik"
  }
}

