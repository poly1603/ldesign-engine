{
  "name": "@ldesign/engine-core",
  "version": "0.3.0",
  "description": "Framework-agnostic core engine - Plugin system, middleware, lifecycle, and more",
  "keywords": [
    "engine",
    "core",
    "plugin",
    "middleware",
    "framework-agnostic",
    "typescript"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./config": {
      "types": "./es/config/index.d.ts",
      "import": "./es/config/index.js",
      "require": "./lib/config/index.cjs"
    },
    "./config/*": {
      "types": "./es/config/*.d.ts",
      "import": "./es/config/*.js",
      "require": "./lib/config/*.cjs"
    },
    "./constants/*": {
      "types": "./es/constants/*.d.ts",
      "import": "./es/constants/*.js",
      "require": "./lib/constants/*.cjs"
    },
    "./container": {
      "types": "./es/container/index.d.ts",
      "import": "./es/container/index.js",
      "require": "./lib/container/index.cjs"
    },
    "./container/*": {
      "types": "./es/container/*.d.ts",
      "import": "./es/container/*.js",
      "require": "./lib/container/*.cjs"
    },
    "./engine": {
      "types": "./es/engine/index.d.ts",
      "import": "./es/engine/index.js",
      "require": "./lib/engine/index.cjs"
    },
    "./engine/*": {
      "types": "./es/engine/*.d.ts",
      "import": "./es/engine/*.js",
      "require": "./lib/engine/*.cjs"
    },
    "./event": {
      "types": "./es/event/index.d.ts",
      "import": "./es/event/index.js",
      "require": "./lib/event/index.cjs"
    },
    "./event/*": {
      "types": "./es/event/*.d.ts",
      "import": "./es/event/*.js",
      "require": "./lib/event/*.cjs"
    },
    "./lifecycle": {
      "types": "./es/lifecycle/index.d.ts",
      "import": "./es/lifecycle/index.js",
      "require": "./lib/lifecycle/index.cjs"
    },
    "./lifecycle/*": {
      "types": "./es/lifecycle/*.d.ts",
      "import": "./es/lifecycle/*.js",
      "require": "./lib/lifecycle/*.cjs"
    },
    "./middleware": {
      "types": "./es/middleware/index.d.ts",
      "import": "./es/middleware/index.js",
      "require": "./lib/middleware/index.cjs"
    },
    "./middleware/*": {
      "types": "./es/middleware/*.d.ts",
      "import": "./es/middleware/*.js",
      "require": "./lib/middleware/*.cjs"
    },
    "./plugin": {
      "types": "./es/plugin/index.d.ts",
      "import": "./es/plugin/index.js",
      "require": "./lib/plugin/index.cjs"
    },
    "./plugin/*": {
      "types": "./es/plugin/*.d.ts",
      "import": "./es/plugin/*.js",
      "require": "./lib/plugin/*.cjs"
    },
    "./state": {
      "types": "./es/state/index.d.ts",
      "import": "./es/state/index.js",
      "require": "./lib/state/index.cjs"
    },
    "./state/*": {
      "types": "./es/state/*.d.ts",
      "import": "./es/state/*.js",
      "require": "./lib/state/*.cjs"
    },
    "./types": {
      "types": "./es/types/index.d.ts",
      "import": "./es/types/index.js",
      "require": "./lib/types/index.cjs"
    },
    "./types/*": {
      "types": "./es/types/*.d.ts",
      "import": "./es/types/*.js",
      "require": "./lib/types/*.cjs"
    },
    "./__tests__/*": {
      "types": "./es/__tests__/*.d.ts",
      "import": "./es/__tests__/*.js",
      "require": "./lib/__tests__/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "dev": "ldesign-builder build --watch",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  }
}