{
  "name": "@ldesign/engine-core",
  "version": "0.1.0",
  "description": "Framework-agnostic core engine with plugin system, middleware, lifecycle, and more",
  "keywords": [
    "engine",
    "plugin",
    "middleware",
    "framework-agnostic",
    "typescript",
    "lifecycle",
    "events"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./cache": {
      "types": "./es/cache/index.d.ts",
      "import": "./es/cache/index.js",
      "require": "./lib/cache/index.cjs"
    },
    "./cache/*": {
      "types": "./es/cache/*.d.ts",
      "import": "./es/cache/*.js",
      "require": "./lib/cache/*.cjs"
    },
    "./config": {
      "types": "./es/config/index.d.ts",
      "import": "./es/config/index.js",
      "require": "./lib/config/index.cjs"
    },
    "./config/*": {
      "types": "./es/config/*.d.ts",
      "import": "./es/config/*.js",
      "require": "./lib/config/*.cjs"
    },
    "./di": {
      "types": "./es/di/index.d.ts",
      "import": "./es/di/index.js",
      "require": "./lib/di/index.cjs"
    },
    "./di/*": {
      "types": "./es/di/*.d.ts",
      "import": "./es/di/*.js",
      "require": "./lib/di/*.cjs"
    },
    "./events": {
      "types": "./es/events/index.d.ts",
      "import": "./es/events/index.js",
      "require": "./lib/events/index.cjs"
    },
    "./events/*": {
      "types": "./es/events/*.d.ts",
      "import": "./es/events/*.js",
      "require": "./lib/events/*.cjs"
    },
    "./lifecycle": {
      "types": "./es/lifecycle/index.d.ts",
      "import": "./es/lifecycle/index.js",
      "require": "./lib/lifecycle/index.cjs"
    },
    "./lifecycle/*": {
      "types": "./es/lifecycle/*.d.ts",
      "import": "./es/lifecycle/*.js",
      "require": "./lib/lifecycle/*.cjs"
    },
    "./logger": {
      "types": "./es/logger/index.d.ts",
      "import": "./es/logger/index.js",
      "require": "./lib/logger/index.cjs"
    },
    "./logger/*": {
      "types": "./es/logger/*.d.ts",
      "import": "./es/logger/*.js",
      "require": "./lib/logger/*.cjs"
    },
    "./middleware": {
      "types": "./es/middleware/index.d.ts",
      "import": "./es/middleware/index.js",
      "require": "./lib/middleware/index.cjs"
    },
    "./middleware/*": {
      "types": "./es/middleware/*.d.ts",
      "import": "./es/middleware/*.js",
      "require": "./lib/middleware/*.cjs"
    },
    "./plugin": {
      "types": "./es/plugin/index.d.ts",
      "import": "./es/plugin/index.js",
      "require": "./lib/plugin/index.cjs"
    },
    "./plugin/*": {
      "types": "./es/plugin/*.d.ts",
      "import": "./es/plugin/*.js",
      "require": "./lib/plugin/*.cjs"
    },
    "./state": {
      "types": "./es/state/index.d.ts",
      "import": "./es/state/index.js",
      "require": "./lib/state/index.cjs"
    },
    "./state/*": {
      "types": "./es/state/*.d.ts",
      "import": "./es/state/*.js",
      "require": "./lib/state/*.cjs"
    },
    "./types": {
      "types": "./es/types/index.d.ts",
      "import": "./es/types/index.js",
      "require": "./lib/types/index.cjs"
    },
    "./types/*": {
      "types": "./es/types/*.d.ts",
      "import": "./es/types/*.js",
      "require": "./lib/types/*.cjs"
    },
    "./utils": {
      "types": "./es/utils/index.d.ts",
      "import": "./es/utils/index.js",
      "require": "./lib/utils/index.cjs"
    },
    "./utils/*": {
      "types": "./es/utils/*.d.ts",
      "import": "./es/utils/*.js",
      "require": "./lib/utils/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs,dts",
    "dev": "ldesign-builder build -f esm,cjs,dts --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4",
    "eslint": "^9.18.0"
  }
}