{
  "name": "@ldesign/engine-astro",
  "version": "0.1.0",
  "description": "Astro adapter for LDesign Engine",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "pnpm run clean && tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch"
  },
  "keywords": ["ldesign", "engine", "astro", "adapter"],
  "license": "MIT",
  "peerDependencies": {
    "astro": "^4.0.0",
    "@ldesign/engine-core": "workspace:*"
  },
  "devDependencies": {
    "astro": "^5.0.5",
    "@ldesign/engine-core": "workspace:*",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}

