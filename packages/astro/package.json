{
  "name": "@ldesign/engine-astro",
  "version": "0.1.0",
  "description": "Astro adapter for @ldesign/engine-core",
  "keywords": [
    "astro",
    "engine",
    "plugin",
    "middleware",
    "typescript"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./adapter": {
      "types": "./es/adapter/index.d.ts",
      "import": "./es/adapter/index.js",
      "require": "./lib/adapter/index.cjs"
    },
    "./adapter/*": {
      "types": "./es/adapter/*.d.ts",
      "import": "./es/adapter/*.js",
      "require": "./lib/adapter/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs,umd,dts",
    "dev": "ldesign-builder build -f esm,cjs,umd,dts --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ldesign/engine-core": "workspace:*"
  },
  "peerDependencies": {
    "astro": "^4.0.0 || ^5.0.0",
    "@ldesign/engine-core": "workspace:*"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4",
    "astro": "^5.0.0",
    "eslint": "^9.18.0"
  }
}
