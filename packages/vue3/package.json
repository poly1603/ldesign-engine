{
  "name": "@ldesign/engine-vue3",
  "version": "0.3.0",
  "description": "Vue 3 adapter for LDesign Engine",
  "keywords": [
    "vue",
    "vue3",
    "engine",
    "adapter",
    "plugin",
    "middleware"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./composables": {
      "types": "./es/composables/index.d.ts",
      "import": "./es/composables/index.js",
      "require": "./lib/composables/index.cjs"
    },
    "./composables/*": {
      "types": "./es/composables/*.d.ts",
      "import": "./es/composables/*.js",
      "require": "./lib/composables/*.cjs"
    },
    "./engine/*": {
      "types": "./es/engine/*.d.ts",
      "import": "./es/engine/*.js",
      "require": "./lib/engine/*.cjs"
    },
    "./plugins": {
      "types": "./es/plugins/index.d.ts",
      "import": "./es/plugins/index.js",
      "require": "./lib/plugins/index.cjs"
    },
    "./plugins/*": {
      "types": "./es/plugins/*.d.ts",
      "import": "./es/plugins/*.js",
      "require": "./lib/plugins/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "dev": "ldesign-builder build --watch",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@ldesign/engine-core": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.3.0 || ^3.4.0 || ^3.5.0",
    "@ldesign/router-vue": "workspace:*",
    "@ldesign/i18n-vue": "workspace:*"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@ldesign/i18n-vue": "workspace:*",
    "@ldesign/router-vue": "workspace:*",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.18"
  },
  "peerDependenciesMeta": {
    "@ldesign/router-vue": {
      "optional": true
    },
    "@ldesign/i18n-vue": {
      "optional": true
    }
  }
}