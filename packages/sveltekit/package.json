{
  "name": "@ldesign/engine-sveltekit",
  "version": "0.1.0",
  "description": "SvelteKit adapter for @ldesign/engine-core",
  "keywords": [
    "sveltekit",
    "svelte",
    "engine",
    "plugin",
    "middleware",
    "typescript"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs,umd,dts",
    "dev": "ldesign-builder build -f esm,cjs,umd,dts --watch",
    "test": "vitest",
    "lint": "eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ldesign/engine-core": "workspace:*",
    "@ldesign/engine-svelte": "workspace:*"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^4.0.0",
    "@ldesign/engine-core": "workspace:*"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4",
    "@sveltejs/kit": "^2.5.0",
    "svelte": "^4.2.0",
    "eslint": "^9.18.0"
  }
}
