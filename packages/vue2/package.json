{
  "name": "@ldesign/engine-vue2",
  "version": "0.3.0",
  "description": "Vue 2 adapter for LDesign Engine",
  "keywords": [
    "vue",
    "vue2",
    "engine",
    "adapter",
    "plugin",
    "middleware"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./adapters/*": {
      "types": "./es/adapters/*.d.ts",
      "import": "./es/adapters/*.js",
      "require": "./lib/adapters/*.cjs"
    },
    "./core/*": {
      "types": "./es/core/*.d.ts",
      "import": "./es/core/*.js",
      "require": "./lib/core/*.cjs"
    },
    "./utils/*": {
      "types": "./es/utils/*.d.ts",
      "import": "./es/utils/*.js",
      "require": "./lib/utils/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "dev": "ldesign-builder build --watch",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@ldesign/engine-core": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^2.6.0 || ^2.7.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@ldesign/router": "workspace:*",
    "@ldesign/router-vue2": "workspace:*",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4",
    "vue": "^2.7.16"
  },
  "optionalDependencies": {
    "@ldesign/router": "workspace:*",
    "@ldesign/router-vue2": "workspace:*"
  }
}