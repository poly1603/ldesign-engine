  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs,umd",
    "build:clean": "node scripts/build.js --clean",
    "build:types": "node scripts/build.js --types",
    "build:analyze": "node scripts/analyze-bundle.js",
    "build:watch": "rollup -c -w",
    "build:check": "pnpm run build && pnpm run build:analyze",
    "dev": "rollup -c -w",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "vitest",
    "test:unit": "vitest run tests/utils --reporter=verbose --coverage",
    "test:integration": "vitest run tests/integration --reporter=verbose",
    "test:performance": "vitest run tests/performance --reporter=verbose --testTimeout=60000",
    "test:benchmark": "vitest run --config=vitest.benchmark.config.ts",
    "test:benchmark:watch": "vitest --config=vitest.benchmark.config.ts --watch",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "pnpm run test:unit && pnpm run test:integration && pnpm run test:performance && pnpm run test:e2e",
    "clean": "rimraf esm cjs dist types lib coverage .nyc_output",
    "size-check": "size-limit",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run test:run",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "dev:examples": "vite serve examples --port 5173",
    "serve:examples": "vite preview examples --port 5173",
    "ci": "pnpm run lint:check && pnpm run format:check && pnpm run type-check && pnpm run test:all",
    "precommit": "pnpm run lint && pnpm run format && pnpm run type-check",
    "security:audit": "pnpm audit",
    "security:fix": "pnpm audit --fix",
    "build:dev": "node scripts/build.js --dev",
    "test:watch": "vitest --watch",
    "test:performance:watch": "vitest tests/performance --watch --testTimeout=60000",
    "analyze:bundle": "pnpm run build:analyze",
    "analyze:performance": "pnpm run test:benchmark",
    "quality:check": "pnpm run lint:check && pnpm run format:check && pnpm run type-check && pnpm run test:unit",
    "quality:fix": "pnpm run lint && pnpm run format && pnpm run test:unit"
  },
  "peerDependencies": {
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/ui": "^3.2.4",
    "@vue/compiler-dom": "^3.5.18",
    "@vue/runtime-dom": "^3.5.18",
    "@vue/shared": "^3.5.18",
    "@vue/test-utils": "^2.4.4",
    "bundle-analyzer": "^0.0.6",
    "eslint": "^9.0.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "size-limit": "^11.0.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.0.12",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4",
    "vue": "^3.5.18",
    "vue-tsc": "^3.0.5",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "50 KB"
    }
  ],
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  }
}